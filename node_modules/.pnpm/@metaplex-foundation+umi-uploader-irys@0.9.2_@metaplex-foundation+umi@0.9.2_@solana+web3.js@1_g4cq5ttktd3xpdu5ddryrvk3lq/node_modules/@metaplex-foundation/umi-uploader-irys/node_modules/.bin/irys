  export NODE_PATH="/Users/islaperfito/solana-escrow/node_modules/.pnpm/@irys+sdk@0.0.2_arweave@1.15.5_bufferutil@4.0.9_utf-8-validate@5.0.10/node_modules/@irys/sdk/build/cjs/node/node_modules:/Users/islaperfito/solana-escrow/node_modules/.pnpm/@irys+sdk@0.0.2_arweave@1.15.5_bufferutil@4.0.9_utf-8-validate@5.0.10/node_modules/@irys/sdk/build/cjs/node_modules:/Users/islaperfito/solana-escrow/node_modules/.pnpm/@irys+sdk@0.0.2_arweave@1.15.5_bufferutil@4.0.9_utf-8-validate@5.0.10/node_modules/@irys/sdk/build/node_modules:/Users/islaperfito/solana-escrow/node_modules/.pnpm/@irys+sdk@0.0.2_arweave@1.15.5_bufferutil@4.0.9_utf-8-validate@5.0.10/node_modules/@irys/sdk/node_modules:/Users/islaperfito/solana-escrow/node_modules/.pnpm/@irys+sdk@0.0.2_arweave@1.15.5_bufferutil@4.0.9_utf-8-validate@5.0.10/node_modules/@irys/node_modules:/Users/islaperfito/solana-escrow/node_modules/.pnpm/@irys+sdk@0.0.2_arweave@1.15.5_bufferutil@4.0.9_utf-8-validate@5.0.10/node_modules:/Users/islaperfito/solana-escrow/node_modules/.pnpm/node_modules:$NODE_PATH"
