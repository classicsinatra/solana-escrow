  exec "$basedir/node"  "$basedir/../../../../../../@irys+sdk@0.0.2_arweave@1.15.5_bufferutil@4.0.9_utf-8-validate@5.0.10/node_modules/@irys/sdk/build/cjs/node/cli.js" "$@"
